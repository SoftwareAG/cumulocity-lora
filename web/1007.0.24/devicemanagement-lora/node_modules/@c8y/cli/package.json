{
  "name": "@c8y/cli",
  "version": "1007.0.32",
  "license": "Apache-2.0",
  "author": "Cumulocity",
  "description": "CLI tool for Cumulocity IoT applications",
  "main": "cli.js",
  "engines": {
    "node": ">=10"
  },
  "bin": {
    "c8ycli": "cli.js"
  },
  "scripts": {
    "build": "tsc -p . && tsc -p src/app-bootstrap && npm run copy:schemas",
    "copy:schemas": "ncp src/ng-cli/schemas dist/ng-cli/schemas",
    "lint": "tslint --project tsconfig.json",
    "prepare": "npm run build",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test": "jest",
    "test:e2e": "node __tests__/index.js",
    "watch": "tsc -w",
    "watch:bootstrap": "tsc -w -p src/app-bootstrap"
  },
  "builders": "./dist/ng-cli/schemas/builders.json",
  "schematics": "./dist/ng-cli/schemas/collection.json",
  "keywords": [
    "Cumulocity",
    "IoT",
    "m2m",
    "cli",
    "tools"
  ],
  "dependencies": {
    "@babel/core": "7.7.7",
    "@babel/plugin-proposal-object-rest-spread": "7.4.3",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.7.6",
    "@c8y/client": "1007.0.32",
    "JSONPath": "0.11.2",
    "angular-gettext-tools": "2.4.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-plugin-angularjs-annotate": "0.10.0",
    "chalk": "2.4.1",
    "commander": "2.15.1",
    "core-js": "^3.4.0",
    "css-loader": "2.1.1",
    "css-vars-ponyfill": "^1.15.3",
    "es6-promise": "4.2.5",
    "express": "4.16.4",
    "fast-glob": "2.2.0",
    "fs-extra": "5.0.0",
    "html-webpack-plugin": "3.2.0",
    "http-proxy-middleware": "0.19.1",
    "imports-loader": "^0.8.0",
    "less": "^3.8.0",
    "less-loader": "5.0.0",
    "lodash": "4.17.12",
    "log-update": "2.3.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pacote": "9.1.0",
    "pofile": "1.0.11",
    "prompt": "1.0.0",
    "sanitize-filename": "1.6.1",
    "tslib": "^1.9.2",
    "unzip-stream": "0.3.0",
    "webpack-dev-middleware": "3.2.0",
    "webpack-hot-middleware": "2.22.3",
    "webpack-merge": "^4.1.4",
    "webpack-sources": "^1.2.0",
    "yargs": "12.0.2",
    "zip-dir": "1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.7.7",
    "@types/express": "4.17.0",
    "@types/fs-extra": "5.0.1",
    "@types/node": "10.9.4",
    "@types/webpack": "^4.32.1",
    "@types/webpack-dev-server": "^3.4.0",
    "tslint": "^5.9.1",
    "typescript": "3.5.3"
  },
  "peerDependencies": {
    "@angular-devkit/build-angular": "0.803.23"
  }
}